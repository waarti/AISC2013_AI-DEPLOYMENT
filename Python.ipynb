   "source": [
    "#Print basic python code\n",
    "\n",
    "def factorial(n):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n-1)\n",
    "\n",
    "result = factorial(7)\n",
    "print(\"Factorial of 7 is:\", result)"
   ]
